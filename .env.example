# Copy this to .env and fill in your actual keys
# NEVER commit .env to git!

# Required: OpenAI API (for LLM-based security analysis)
OPENAI_API_KEY=your_openai_api_key_here

# Required: Search APIs (for research)
TAVILY_API_KEY=your_tavily_api_key_here
SERPER_API_KEY=your_serper_api_key_here

# Optional: 네이버 검색 API (한글 자료 검색용, HITL 기능에서 사용)
# 발급 방법: https://developers.naver.com/products/search/
# 1. 네이버 개발자 센터 접속
# 2. 애플리케이션 등록
# 3. 검색 API 사용 설정
# 4. Client ID와 Client Secret 복사
NAVER_CLIENT_ID=your_naver_client_id_here
NAVER_CLIENT_SECRET=your_naver_client_secret_here

# Optional: LangSmith tracing (for debugging LangGraph workflows)
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=pushguardian
